{
  "name": "yui-configger",
  "version": "0.5.1",
  "description": "Extract meta-data from a folder of YUI modules & generate a Loader config",
  "usage": "Generate a YUI config.\nUsage: $0 --root=[dir] [dir],..,[dirN]",
  "preferGlobal": true,
  "author": "Pat Cavit <pcavit@gmail.com",
  "bugs": "https://github.com/tivac/yui-configger/issues",
  "bin": {
    "configger": "./bin/cli.js"
  },
  "main": "./lib/configger.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tivac/yui-configger.git"
  },
  "dependencies": {
    "escape-regexp-component": "1.0.x",
    "escodegen": "~1.7.0",
    "esprima": "2.7.0",
    "esquery": "~0.4.0",
    "estraverse": "~4.1.0",
    "globule": "~0.2.0",
    "lodash": "~3.10.1",
    "npmlog": "2.0.0",
    "optimist": "0.6.x"
  },
  "devDependencies": {
    "istanbul": "~0.4.0",
    "jscs": "~2.7.0",
    "jshint": "~2.8.0",
    "mocha": "~2.3.3",
    "nixt": "~0.4.1"
  },
  "scripts": {
    "test": "npm run-script jshint && npm run-script istanbul",
    "jshint": "jshint .",
    "jscs": "jscs -c ./.jscsrc lib/",
    "istanbul": "istanbul test --print both ./node_modules/mocha/bin/_mocha",
    "mocha": "mocha"
  },
  "license": "MIT",
  "engine": {
    "node": ">=0.8"
  },
  "keywords": [
    "yui",
    "config",
    "esprima"
  ]
}
